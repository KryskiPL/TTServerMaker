<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:math="http://hexinnovation.com/math"
                    xmlns:converters="clr-namespace:TTServerMaker.WPF.Converters">
    
    <converters:HasSelectedItemConverter x:Key="HasSelectedItemConverter" />
    <converters:BoolToNegatedBoolConverter x:Key="BoolToNegatedBoolConverter" />
    <converters:BoolToVisibilityConverter x:Key="BooleanToHiddenConverter" TrueValue="Visible" FalseValue="Hidden" />
    <converters:StringToBooleanConverter x:Key="StringToBooleanConverter" />
    <converters:NullToBoolConverter x:Key="NullToBooleanConverter" />

    <math:MathConverter x:Key="Math" />
</ResourceDictionary>
